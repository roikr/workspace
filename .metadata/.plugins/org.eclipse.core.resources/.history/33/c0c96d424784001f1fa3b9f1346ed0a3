		[Embed(source="../sounds/steam_01.mp3")] public static var CLOUDS_SOUND_STEAM_1:Class;
		
		
		public function SoundsLibrary()
  		{
    		//var str:String =  getQualifiedClassName(sn2_4_000);
    		//var name:String = "sn2_4_000";
    		
  		}
  		
  		public function getClass(name:String) : Class {
  			return  getDefinitionByName("SoundsLibrary"+name) as Class;
    		
  		}
	}
