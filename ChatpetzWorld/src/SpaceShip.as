package {	import flash.display.MovieClip;	import flash.events.Event;	public class SpaceShip extends MovieClip {				private var client:Object;								public function SpaceShip() : void {			addEventListener(Event.ENTER_FRAME,onEnterFrame);		}				public function set gameClient(client:Object) : void {			this.client = client;		}				private function onEnterFrame(e:Event) : void {			if (client && currentFrameLabel) {					client.onSpaceShipFrameLabel(currentFrameLabel);			}		}								}}							